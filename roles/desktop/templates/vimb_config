shortcut-add gg=https://www.google.co.uk/#q=$0
shortcut-add gh=https://www.github.com/$0/$1
shortcut-add php=http://php.net/manual-lookup.php?pattern=$0
shortcut-add wikipedia=http://en.wikipedia.org/wiki/$0
shortcut-add wiktionary=http://en.wiktionary.org/wiki/$0
shortcut-add dict=http://en.wiktionary.org/wiki/$0
shortcut-add d=http://en.wiktionary.org/wiki/$0
shortcut-add packagist=https://packagist.org/search?q=$0
shortcut-add pirate=https://oldpiratebay.org/search?q=$0&sort=seeders

set editor-command=gvim -f %s
set webinspector=on
set download-path="~/Downloads"

nnoremap H <C-O>
nnoremap L <C-I>

