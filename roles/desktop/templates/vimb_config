shortcut-add gg=https://www.google.co.uk/?q=$0#q=$0
shortcut-add gh=https://www.github.com/$0/$1
shortcut-add sulu=https://www.github.com/sulu-cmf/$0
shortcut-add cmf=https://www.github.com/symfony-cmf/$0
shortcut-add php=http://php.net/manual-lookup.php?pattern=$0

set editor-command=gvim -f %s

nnoremap H <C-O>
nnoremap L <C-I>
